{
  "name": "@epic-web/workshop-app",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "parallel": true,
        "cwd": "packages/workshop-app",
        "commands": [
          "npm run build:remix",
          "npm run build:server",
          "npm run build:utils",
          "npm run build:decs"
        ]
      }
    }
  }
}
